    "temp = fullCost.copy()\n",
    "del temp['비용구분']\n",
    "temp.columns = temp.columns.str.split('_').str[0]+'4분기'\n",
    "temp = temp.T\n",
    "temp = temp*1000\n",
    "\n",
    "sales4Qdata = sales.loc[sales['분기']=='4분기']\n",
    "sales4Qdata['구분'] = sales4Qdata['연도']+'년'+sales4Qdata['분기']\n",
    "sales4Qdata = sales4Qdata[['구분','당기금액']]\n",
    "sales4Qdata = sales4Qdata.set_index('구분')\n",
    "sales4Qdata.columns = ['매출액']\n",
    "sales4Qdata['변동비'] = temp['변동비합계']\n",
    "sales4Qdata['공헌이익'] = sales4Qdata['매출액']-sales4Qdata['변동비']\n",
    "sales4Qdata['고정비'] = temp['고정비합계']\n",
    "sales4Qdata['영업이익'] = sales4Qdata['공헌이익']-sales4Qdata['고정비']\n",
    "sales4Qdata['영업레버리지'] = sales4Qdata['공헌이익']/sales4Qdata['영업이익']\n",
    "doldata = sales4Qdata.copy()\n",
    "for column in doldata.columns : doldata[column] = doldata[column].map('{:,.2f}'.format)\n",
    "display(doldata)"
   ]
