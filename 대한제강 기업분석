   "source": [
    "sales = fs.loc[(fs['계정명'] == '매출액') & (fs['연결개별명'] == '연결'),['연도','분기','당기금액','당기누적금액']]\n",
    "sales = sales.replace('',0)\n",
    "sales[['당기금액','당기누적금액']] = sales[['당기금액','당기누적금액']].astype('int')\n",
    "sales['누적참조'] = sales['당기누적금액'].shift(1)\n",
    "sales['매출액'] = sales['당기금액']\n",
    "\n",
    "salesDf = sales.copy()\n",
    "salesDf.loc[salesDf['분기']=='4분기','매출액'] = salesDf.loc[salesDf['분기']=='4분기','당기금액'] - salesDf.loc[salesDf['분기']=='4분기','누적참조'] \n",
    "salesDf['구분'] = salesDf['연도']+'년'+salesDf['분기']\n",
    "salesDf = salesDf[['구분','매출액']]\n",
    "salesDf = salesDf.set_index('구분')\n",
    "salesDf = salesDf.dropna()\n",
    "salesDf['매출액참조'] = salesDf['매출액'].shift(1)\n",
    "salesDf[['매출액','매출액참조']] = salesDf[['매출액','매출액참조']].astype('float')\n",
    "salesDf['매출액증가율'] = salesDf['매출액']/salesDf['매출액참조']\n",
    "salesDf['매출액'] = salesDf['매출액'].astype('int')\n",
    "salesDf = salesDf[['매출액','매출액증가율']]\n",
    "salesDf['매출액지수'] = salesDf['매출액'] / salesDf['매출액'][0]\n",
    "salesDf"
   ]
  },
